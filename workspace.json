{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/next"
  },
  "defaultProject": "blog",
  "generators": {
    "@nrwl/react": {
      "application": {
        "babel": true
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "styled-components",
        "linter": "eslint"
      }
    }
  },
  "projects": {
    "blog": {
      "root": "apps/blog",
      "sourceRoot": "apps/blog",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "root": "apps/blog",
            "outputPath": "dist/apps/blog"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "blog:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "blog:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "blog:build:production"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/blog"
          ],
          "options": {
            "jestConfig": "apps/blog/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/blog/**/*.{ts,tsx,js,jsx}"
            ]
          }
        }
      }
    },
    "blog-e2e": {
      "root": "apps/blog-e2e",
      "sourceRoot": "apps/blog-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/blog-e2e/cypress.json",
            "tsConfig": "apps/blog-e2e/tsconfig.e2e.json",
            "devServerTarget": "blog:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "blog:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/blog-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
    "infrastructure": {
      "root": "apps/infrastructure",
      "sourceRoot": "apps/infrastructure/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/infrastructure",
            "main": "apps/infrastructure/src/main.ts",
            "tsConfig": "apps/infrastructure/tsconfig.app.json",
            "assets": [
              "apps/infrastructure/src/assets"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/infrastructure/src/environments/environment.ts",
                  "with": "apps/infrastructure/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "infrastructure:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/infrastructure/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/infrastructure"
          ],
          "options": {
            "jestConfig": "apps/infrastructure/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}